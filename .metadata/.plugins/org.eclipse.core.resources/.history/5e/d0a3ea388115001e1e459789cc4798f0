<button type="button">click</button>

<script type="text/javascript">
$(document).ready(function(){
	$("button").click(function(){
		$.ajax({
			url:"user", //webxml
			type:"get",
		//	data:"id=abc&pw=qwe",
			data:{"id":"abc","pw":"123"},
			success:function(resp){
				//alert('success');
				//alert(JSON.stringify(resp));
				alert(resp.str);
			},
			error : function(){
				alert('error')
			}
		})
	});	
})
</script>
