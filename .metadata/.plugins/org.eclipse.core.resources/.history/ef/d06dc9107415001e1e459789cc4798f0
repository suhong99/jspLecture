<script type="text/javascript">
$(document).ready(function(){
	$("button").click(function(){
		$.ajax({
			url:"data.jsp",
			type:"get",   //get//post
			data:"t1=abc&t2=가나다",
			success : function (resp){
				alert("success")
			},
			error: function(){
				alert("error")

			},
			complete:function(){
				alert("통신종료")

			}
		});
	});
})
</script>
